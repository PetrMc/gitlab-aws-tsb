---
apiVersion: traffic.tsb.tetrate.io/v2
kind: ServiceRoute
metadata:
  name: tetrate-app
  group: blog-test-traffic-group
  workspace: gitlab-my-apps-ws
  tenant: tetrate
spec:
  service: tetrate-demo/tetrate-app.tetrate-demo.svc.cluster.local
  subsets:
  - name: init_version
    labels:
      version: v${INIT_VERSION}
    weight: ${INIT_WEIGHT}
  - name: new_version
    labels:
      version: v${APP_VERSION}
    weight: ${NEW_WEIGHT}